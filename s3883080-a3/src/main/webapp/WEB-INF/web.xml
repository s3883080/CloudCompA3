<?xml version="1.0" encoding="utf-8"?>
<web-app
        xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
         http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">

  <servlet>
    <servlet-name>LoginServlet</servlet-name>
    <servlet-class>com.amazonaws.s3883080.LoginServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LoginServlet</servlet-name>
    <url-pattern>/loginServlet</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>AddPhotoServlet</servlet-name>
    <servlet-class>com.amazonaws.s3883080.AddPhotoServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>AddPhotoServlet</servlet-name>
    <url-pattern>/addPhotoServlet</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>RegistrationServlet</servlet-name>
    <servlet-class>com.amazonaws.s3883080.RegistrationServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>RegistrationServlet</servlet-name>
    <url-pattern>/registrationServlet</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>BuyServlet</servlet-name>
    <servlet-class>com.amazonaws.s3883080.BuyServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>BuyServlet</servlet-name>
    <url-pattern>/buyServlet</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>CompletePurchaseServlet</servlet-name>
    <servlet-class>com.amazonaws.s3883080.CompletePurchaseServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>CompletePurchaseServlet</servlet-name>
    <url-pattern>/completePurchaseServlet</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>PostInstrumentServlet</servlet-name>
    <servlet-class>com.amazonaws.s3883080.PostInstrumentServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>PostInstrumentServlet</servlet-name>
    <url-pattern>/postInstrumentServlet</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>LogoutServlet</servlet-name>
    <servlet-class>com.amazonaws.s3883080.LogoutServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LogoutServlet</servlet-name>
    <url-pattern>/logoutServlet</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>ProcessInstrumentServlet</servlet-name>
    <servlet-class>com.amazonaws.s3883080.ProcessInstrumentServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ProcessInstrumentServlet</servlet-name>
    <url-pattern>/processInstrumentServlet</url-pattern>
  </servlet-mapping>

  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
</web-app>
